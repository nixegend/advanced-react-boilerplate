{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "basic react starter, dynamically loading modules",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./server/index.js --open",
    "build": "webpack --prod",
    "dev": "node ./dev.starter.js",
    "karma:test": "karma start",
    "watch:test": "npm test -- -w",
    "test": "mocha src/**/*.test.js --compilers js:babel-register --colors",
    "cover": "babel-istanbul cover --report text --report html --dir ./coverage node_modules/mocha/bin/_mocha src/**/*.test.js --colors",
    "lint": "eslint src",
    "clean": "rm -r static/app/*"
  },
  "engines": {
    "node": "6.2.1",
    "npm": "3.8.6"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
