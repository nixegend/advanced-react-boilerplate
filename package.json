    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "nock": "^8.0.0",
    "chai": "^3.5.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.20.2",
    "flux-standard-action": "^0.6.1",
