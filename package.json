{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "react starter, dynamically loading modules",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./server/index.js --open",
    "build": "webpack --prod",
    "dev": "node ./dev.starter.js",
    "karma:test": "karma start",
    "watch:test": "npm t -- -w",
    "test": "mocha -t 15000 \"src/**/*.test.js\" --compilers js:babel-register --require ignore-styles -c",
    "cover": "babel-istanbul cover --report text --report html --colors --dir ./coverage node_modules/mocha/bin/_mocha -- -t 15000 \"src/**/*.test.js\" --require ignore-styles -c",
    "lint": "eslint src --color",
    "clean": "rm -r static/app/*"
  },
  "engines": {
    "node": "6.2.1",
    "npm": "3.8.6"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "react": "^15.0.1",
    "isomorphic-fetch": "^2.2.1",
    "react-dom": "^15.0.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux-api-middleware": "^1.0.2"
  },
